//class is creating here//
class car  {
    constructor(brand, year,color,model){
        this.brand = brand;
        this.year = year;
        this.color = color;
        this.model = model;
        
    }
    displayinfo(){
        return `${this.brand} was made in ${this.year} color is ${this.color} and car model is the ${this.model} company is genrated here ok `;
        
    }
}

//object is creating here
const mycar = new car('Toyota', 'balck', 'Bs6',2020);
console.log(mycar.displayinfo());
